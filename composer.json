{
    "name": "idct/sftp-client",
    "description": "Library that provides wrapper methods around SSH2 and SFTP to simplify file download/upload over SSH/SCP/SFTP.",
    "type": "library",
    "keywords": [
        "sftp",
        "ssh",
        "ssh2",
        "scp",
        "idct"
    ],
    "homepage": "https://github.com/ideaconnect/idct-sftp-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Bartosz PachoÅ‚ek",
            "email": "bartosz@idct.pl",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/ideaconnect/idct-sftp-client/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "ext-ssh2": ">=0.12"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "IDCT\\Networking\\Ssh\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2"
    }
}
